{
  "name": "@l2studio/nhentai-tags",
  "version": "0.1.0",
  "description": "A library for nhentai.net tags",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/l2studio/nhentai-tags.git",
