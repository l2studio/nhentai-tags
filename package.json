    "@types/file-saver": "^2.0.2",
    "@types/node": "^15.12.2",
    "@types/numeral": "^2.0.1",
    "@types/tunnel": "^0.0.2",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "cheerio": "^1.0.0-rc.10",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "dotenv": "^10.0.0",
    "file-saver": "^2.0.5",
    "got": "^11.8.2",
    "numeral": "^2.0.6",
