  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.0"
  },
  "scripts": {
    "clean": "rimraf build dist",
    "dev": "vite --port=8091",
    "sync:tags": "cross-env DEBUG='lgou2w:*' ts-node -T -P tsconfig.json scripts/sync-tags.ts",
    "build:tags": "cross-env DEBUG='lgou2w:*' ts-node -T -P tsconfig.json scripts/build-tags.ts",
    "build": "npm run build:tags && tsc",
    "prepublishOnly": "npm run clean && npm run build"
